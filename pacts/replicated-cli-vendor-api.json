          "app": {
            "channels": [
              {
              },
              {
              },
              {
              }
            ],
            "created": "2000-02-01T12:30:00Z",
            "description": "",
            "id": "",
            "isArchived": false,
            "isKotsApp": true,
            "name": "app-create-1",
            "renamedAt": "2000-02-01T12:30:00Z",
            "slug": "app-create-1",
            "teamId": "replicated-cli-app-create"
          }
        },
        "matchingRules": {
          "$.body.app.channels": {
            "min": 3
          },
          "$.body.app.channels[*].*": {
            "match": "type"
          },
          "$.body.app.created": {
            "match": "regex",
            "regex": "^([\\+-]?\\d{4}(?!\\d{2}\\b))((-?)((0[1-9]|1[0-2])(\\3([12]\\d|0[1-9]|3[01]))?|W([0-4]\\d|5[0-2])(-?[1-7])?|(00[1-9]|0[1-9]\\d|[12]\\d{2}|3([0-5]\\d|6[1-6])))([T\\s]((([01]\\d|2[0-3])((:?)[0-5]\\d)?|24\\:?00)([\\.,]\\d+(?!:))?)?(\\17[0-5]\\d([\\.,]\\d+)?)?([zZ]|([\\+-])([01]\\d|2[0-3]):?([0-5]\\d)?)?)?)?$"
          },
          "$.body.app.renamedAt": {
            "match": "regex",
            "regex": "^([\\+-]?\\d{4}(?!\\d{2}\\b))((-?)((0[1-9]|1[0-2])(\\3([12]\\d|0[1-9]|3[01]))?|W([0-4]\\d|5[0-2])(-?[1-7])?|(00[1-9]|0[1-9]\\d|[12]\\d{2}|3([0-5]\\d|6[1-6])))([T\\s]((([01]\\d|2[0-3])((:?)[0-5]\\d)?|24\\:?00)([\\.,]\\d+(?!:))?)?(\\17[0-5]\\d([\\.,]\\d+)?)?([zZ]|([\\+-])([01]\\d|2[0-3]):?([0-5]\\d)?)?)?)?$"
          }
        }
      }
